    ],
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public",
    "publish": true
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@hono/auth-js": "^1.0.17",
    "@prisma/extension-accelerate": "^2.0.2",
    "@tailwindcss/vite": "^4.1.11",
    "hono": "^4.8.4",
    "hono-party": "^0.0.13",
    "partyserver": "^0.0.72",
    "partysocket": "1.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.9.4",
    "@eslint/js": "9.31.0",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.0.13",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "9.31.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "jsdom": "^26.1.0",
    "partykit": "0.0.115",
    "prisma": "^6.11.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "vite": "7.0.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.3"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "db:migrate": "dotenv -e .dev.vars -- npx prisma migrate dev",
    "db:reset": "dotenv -e .dev.vars -- npx prisma migrate reset",
    "generate": "dotenv -e .dev.vars -- npx prisma generate --no-engine",
    "studio": "dotenv -e .dev.vars -- npx prisma studio",
    "test": "vitest run --no-watch",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui"
  },
  "type": "module"
}
